{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": ["**/.env.*local"],
  "pipeline": {
    "@you-got-bud/calendar#build": {
      "outputs": ["dist"]
    },
    "@you-got-bud/calendar-web#build": {
      "dependsOn": ["^@you-got-bud/calendar#build"],
      "outputs": [".next/**", "!.next/cache/**"]
    },
    "lint": {
      "dependsOn": ["^lint"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    }
  }
}
